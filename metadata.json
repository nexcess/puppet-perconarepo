{
  "name": "nexcess-perconarepo",
  "version": "0.0.1",
  "author": "nexcess",
  "summary": "Install and configure the Percona yum repo",
  "license": "Apache-2.0",
  "source": "https://github.com/nexcess/puppet-perconarepo",
